{
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "files.exclude": {
    ".git/": true,
    "node_modules/": true
  },
  "search.exclude": {
    ".git/": true
  },
  "code-runner.executorMap": {
    "javascript": "cd $dir && node $fileName",
    "ocaml": "cd $dir && ocaml $fileName",
    "c": "cd $dir && gcc $fileName -o test.exe && ./test.exe && echo  && echo ----done---- && rm ./test.exe",
    "cpp": "cd $dir && g++ $fileName -o test.exe && ./test.exe && echo  && echo ----done---- && rm ./test.exe"
  },
  "code-runner.customCommand": "cd $dir && make clean",
  //"code-runner.customCommand": "g++ $fileName -o output/test.exe && ./output/test.exe < ./input/input.txt > ./output/output.txt && echo ----done---- && rm ./output/test.exe",
  "code-runner.terminalRoot": "/mnt/",
  "code-runner.runInTerminal": true,
  "code-runner.executorMapByGlob": {
    "main.ml": "cd $dir; make",
    "*.spec": "cd $dir; cd ..; ./run -pp test/$fileName.spec"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.shell.windows": "C:\\Users\\TheSky\\AppData\\Local\\Microsoft\\WindowsApps\\CanonicalGroupLimited.UbuntuonWindows_79rhkp1fndgsc\\ubuntu.exe",
  "terminal.external.windowsExec": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "breadcrumbs.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.startupEditor": "welcomePage",
  "terminal.explorerKind": "external"
}
